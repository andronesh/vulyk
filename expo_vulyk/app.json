{
	"expo": {
		"name": "vulyk",
		"slug": "vulyk",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "myapp",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"splash": {
			"image": "./assets/images/splash-icon.png",
			"resizeMode": "contain",
			"backgroundColor": "#FFD300"
		},
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "app.dronehive"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#FFD300"
			},
			"package": "app.dronehive",
			"enableFTS": true,
			"useSQLCipher": false
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": ["expo-router", "expo-sqlite"],
		"experiments": {
			"typedRoutes": true
		}
	}
}
